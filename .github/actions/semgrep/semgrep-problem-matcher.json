{
  "problemMatcher": [
    {
      "owner": "semgrep",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+):([^:]+):([^:]+):(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
